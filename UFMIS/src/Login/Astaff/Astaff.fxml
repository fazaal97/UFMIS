<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTextField?>
<?import de.jensd.fx.glyphs.fontawesome.FontAwesomeIconView?>
<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" fx:id="aStaffWindow" prefHeight="400.0" prefWidth="600.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Login.Astaff.AstaffController">
    <stylesheets>
        <URL value="@astaff.css" />
    </stylesheets>
   <children>
      <HBox alignment="CENTER" prefHeight="400.0" prefWidth="600.0">
         <children>
            <VBox prefHeight="300.0" prefWidth="200.0" />
            <VBox alignment="CENTER" prefHeight="200.0" prefWidth="300.0">
               <children>
                  <JFXButton fx:id="buildingView" onAction="#loadBuildingView" prefHeight="50.0" prefWidth="185.0" text="Building View">
                     <VBox.margin>
                        <Insets bottom="40.0" top="40.0" />
                     </VBox.margin>
                     <graphic>
                        <FontAwesomeIconView glyphName="TABLE" size="25" />
                     </graphic>
                  </JFXButton>
                  <JFXButton fx:id="FmoReport" onAction="#loadFmoReport" prefHeight="50.0" prefWidth="182.0" text="FMO Report">
                     <VBox.margin>
                        <Insets bottom="40.0" top="40.0" />
                     </VBox.margin>
                     <graphic>
                        <FontAwesomeIconView glyphName="TABLE" size="25" />
                     </graphic>
                  </JFXButton>
                  <JFXButton fx:id="setting" onAction="#loadSetting" prefHeight="50.0" prefWidth="185.0" text="Setting">
                     <VBox.margin>
                        <Insets bottom="40.0" top="40.0" />
                     </VBox.margin>
                     <graphic>
                        <FontAwesomeIconView glyphName="COG" size="25" />
                     </graphic>
                  </JFXButton>
               </children>
            </VBox>
            <VBox alignment="BOTTOM_CENTER" layoutX="452.0" layoutY="10.0" prefHeight="300.0" prefWidth="200.0">
               <children>
                  <JFXButton fx:id="cancel" cancelButton="true" onAction="#aCancel" prefHeight="31.0" prefWidth="119.0" text="Cancel">
                     <VBox.margin>
                        <Insets bottom="50.0" />
                     </VBox.margin>
                  </JFXButton>
               </children>
            </VBox>
         </children>
      </HBox>
      <JFXTextField layoutX="34.0" layoutY="-74.0" />
   </children>
</AnchorPane>
